    androidx:id="@+id/my_nav"
    app:startDestination="@id/homeFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        androidx:id="@+id/accountFragment"
        androidx:name="com.example.finalapplication.AccountFragment"
        androidx:label="fragment_account"
