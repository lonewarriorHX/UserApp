        androidx:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        androidx:id="@+id/settingsFragment"
        androidx:name="com.example.finalapplication.SettingsFragment"
        androidx:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        androidx:id="@+id/projectFragment"
        androidx:name="com.example.finalapplication.ProjectFragment"
        androidx:label="fragment_project"
